  "scripts": {
    "start": "cross-env PORT=3001 DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "start:prod": "cross-env NODE_ENV=production PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "cross-env CI=true react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,css,md}",
    "start-with-env": "cp .env.example .env.local && npm start",
    "analyze": "npm run build && npx serve -s build",
    "clean": "rm -rf build node_modules/.cache",
    "clean:install": "rm -rf node_modules package-lock.json && npm install",
    "health-check": "curl http://localhost:3001/health || echo 'Frontend health check failed'",
    "pre-commit": "npm run lint && npm run format:check && npm run test:ci"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "prettier": "^3.4.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "react",
    "hotel-management",
    "booking-system",
    "frontend",
    "spa",
    "bootstrap",
    "responsive"
  ],
